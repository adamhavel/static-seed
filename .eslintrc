{
   "env": {
      "browser": true,
      "node": true,
      "es6": true
   },
   "globals": {
      "Modernizr": false,
      "FastClick": false,
      "App": false
   },
   "rules": {
      "eqeqeq": [2, "smart"],
      "quotes": 0,
      "strict": 0,
      "no-unused-vars": [2, {
         "vars": "local"
      }],
      "valid-jsdoc": [2, {
         "requireReturn": false
      }],
      "camelcase": 0,
      "comma-spacing": 0,
      "eol-last": 0,
      "key-spacing": 0,
      "new-cap": 0,
      "no-trailing-spaces": 0,
      "space-infix-ops": 0
   }
}